import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        // TODO code application logic here
       Scanner in= new Scanner (System.in);
       int A,B,C,D;
        A=in.nextInt();
        B=in.nextInt();
        C=in.nextInt();
        D=in.nextInt();
        int DIFERENCA =(A*B-C*D);
        System.out.println("DIFERENCA = "+DIFERENCA);
    }
    
}
