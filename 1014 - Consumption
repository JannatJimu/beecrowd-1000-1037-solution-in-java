import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // TODO code application logic here
        Scanner in = new Scanner (System .in);
        int X;
        double Y;
        X=in.nextInt();
        Y=in.nextDouble();
        double  Average=X/Y;
        System.out.printf("%.3f km/l\n",Average);
    }
    
}
