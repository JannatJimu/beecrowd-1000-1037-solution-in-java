import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
        // TODO code application logic here
    Scanner in = new Scanner (System.in); 
    int A,B,C;
    A=in.nextInt();
    B=in.nextInt();
    C=in.nextInt();
    int max;
    if(A>B &&A>C)
    {
        max=A;
    }
    else
    {
        max=B;
    }
    if(max<C)
    {
         max=C;
    }
        
        System.out.println(max+" eh o maior");
    }
    
}
